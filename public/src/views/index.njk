{% extends  "layout.html" %}
{% block  content %}
<table class = "table">
    <thead>
    <tr>
        <th>Course ID</th>
        <th>Course Name</th>
    </tr>
    </thead>
    <tbody>
    {% for course in courses%}
    <tr>
        <td>{{course.courseID}}</td>
        <td >{{course.courseName}}</td>
    </tr>
    {% endfor %}
    </tbody>
</table>
{% endblock %}